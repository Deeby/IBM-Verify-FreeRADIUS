version: "2"
services:
  isam-freeradius:
    container_name: verify-freeradius
    build:
      context: .
      dockerfile: Dockerfile
    image: "jared/verify-freeradius"
    ports:
      - "0.0.0.0:1812:1812"
      - "0.0.0.0:1812:1812/udp"
    entrypoint:
     - bash
    labels:
     - "IBM Verify FreeRADIUS"
    tty: true
    stdin_open: true
